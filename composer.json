{
  "name": "artisanpack-ui/icons",
  "description": "This package provides a set of icons for use in the ArtisanPack UI as well as a way for developers to add their own icons and to call them based on category and type.",
  "type": "library",
  "license": "MIT",
  "version": "1.0.4",
  "autoload": {
    "psr-4": {
      "ArtisanPackUI\\Icons\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Jacob Martella",
      "email": "me@jacobmartella.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": ">=5.3",
    "tormjens/eventy": "^0.9.4",
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "pestphp/pest": "^3.8",
    "pestphp/pest-plugin-laravel": "^3.1",
    "orchestra/testbench": "^10.2"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArtisanPackUI\\Icons\\IconsServiceProvider"
      ],
      "aliases": {
        "Package": "ArtisanPackUI\\Icons\\Facades\\Icons"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/pest"
  }
}

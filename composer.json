{
  "name": "artisanpack-ui/icons",
  "description": "A custom icon system for ArtisanPack UI.",
  "version": "1.0.0",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12.0",
    "blade-ui-kit/blade-icons": "^1.8",
    "tormjens/eventy": "^0.9.4"
  },
  "require-dev": {
    "orchestra/testbench": "^10.0",
    "pestphp/pest": "^3.5",
    "pestphp/pest-plugin-laravel": "^3.0",
    "mockery/mockery": "^1.6",
    "phpunit/phpunit": "^11.5"
  },
  "autoload": {
    "psr-4": {
      "ArtisanPackUI\\Icons\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "pest"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArtisanPackUI\\Icons\\IconsServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
